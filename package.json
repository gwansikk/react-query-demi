{
  "name": "tanstack-query-demi",
  "version": "0.1.3",
  "description": "Universal Library for TanStack/React-Query",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "postinstall": "node -e \"import('./scripts/postinstall.mjs').catch(e => console.error(e))\"",
    "build": "tsup"
  },
  "files": [
    "dist",
    "scripts"
  ],
  "keywords": [],
  "author": "gwansikk",
  "license": "MIT",
  "packageManager": "pnpm@9.4.0",
  "devDependencies": {
    "@types/node": "^20.14.6",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4 || ^5"
  }
}
